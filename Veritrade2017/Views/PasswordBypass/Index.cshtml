@model  Veritrade2017.Models.FakeCredentials

@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Veritrade Entorno de desarrollo</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>

</head>
<body style="margin-left:5em">
    <container>
        @using (Html.BeginForm("Index", "PasswordBypass", FormMethod.Post))
        {

            <table cellpadding="0" cellspacing="0">
                <tr>
                    <th colspan="2" align="center"><h3>Ingrese nombre y clave</h3></th>
                </tr>
                <tr>
                    <td>nombre: </td>
                    <td style="padding:1em">
                        @Html.TextBoxFor(m => m.nombre)
                    </td>
                </tr>
                <tr>
                    <td>clave: </td>
                    <td style="padding:1em">
                        @Html.PasswordFor(m => m.clave)
                    </td>
                </tr>



            </table>


            <input class="btn btn-primary" type="submit" value="Submit" />

        }



        <div class="container body-content">

            <hr />
            <footer>
            </footer>
        </div>

    </container>
</body>
</html>


