<div class="modal fade" tabindex="-1" role="dialog" id="ModalVentanaSesion">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span class="icon-close-modal" aria-hidden="true">&times;</span>
                </button>
                <h4 class="modal-title"></h4>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-sm-12">
                        <div class="text-center title-ventana-session">
                            @Resources.AdminResources.Modal_VentanaSesion_Title
                        </div>
                        <div class="text-center texto-ventana-session">
                            <p>
                                @Resources.AdminResources.Modal_VentanaSesion_Message
                            </p>
                        </div>
                        <div class="text-center">
                            <button type="button" data-dismiss="modal" class="btn btn-ok">
                                @Resources.AdminResources.Modal_VentanMensaje_OKButton
                            </button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

